{
  "extends": "eslint-config-airbnb-es5",
  "rules": {
    "indent": ["error", 4],
    "space-before-blocks": "off",
    "no-plusplus": "off",
    "func-names": "off"
  },
  "globals": {
    "describe": true,
    "it": true,
    "Set": true,
    "Map": true,
    "spy": true
  },
  "env": {
    "node": true
  }
}